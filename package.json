  "description": "Server for appoinment-ease app. An online platform to make appoinments on Android devices.",
  "main": "./dist/server.js",
  "scripts": {
    "build": "gulp scripts",
    "build-admin": "ng build --prod -pr -op ./dist/views/admin/ -dop",
    "compile": "node node_modules/typescript/lib/tsc.js -p ./tsconfig.json",
    "build2": "npm run compile",
    "rerun": "npm run compile && npm start",
    "start": "NODE_ENV=production node ./dist/server.js",
    "test": "mocha --reporter spec --compilers ts:ts-node/register 'test/**/*.test.ts'",
    "watch": "NODE_ENV=dev nodemon ./dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/davejoem/wims-server.git"
  },
